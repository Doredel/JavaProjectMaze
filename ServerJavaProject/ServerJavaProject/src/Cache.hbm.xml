<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
  <hibernate-mapping>
      <class name="model.db.Cache" table="MAZECACHE" >
          <id name="name" type="java.lang.String" column="NAME" />
          <property name="maze" type="blob" column="MAZE" />
          <property name="solution" type="blob" column="SOLUTION"/>
      </class>
  </hibernate-mapping>
